{
    "name": "get-protocol",
    "version": "1.0.0",
    "description": "A WordPress plugin for GET Protocol, integrating training & events, MCP bridge, and publishing/social syndication.",
    "keywords": [
        "wordpress",
        "get-protocol",
        "training",
        "events",
        "mcp",
        "publishing"
    ],
    "homepage": "https://github.com/your-repo/get-protocol",
    "license": "GPL-2.0-or-later",
    "author": "Your Name",
    "main": "src/index.js",
    "private": true,
    "files": [
        "build",
        "includes",
        "vendor",
        "get-protocol.php",
        "docs"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "format": "prettier --write .",
        "lint:css": "stylelint '**/*.css'",
        "lint:js": "eslint '**/*.js'",
        "lint:php": "phpcs --standard=.phpcs.xml.dist --extensions=php --ignore=vendor,build,docs,node_modules,tests",
        "lint:php:fix": "phpcbf --standard=.phpcs.xml.dist --extensions=php --ignore=vendor,build,docs,node_modules,tests",
        "clean": "rm -rf build"
    },
    "dependencies": {
        "@wordpress/api-fetch": "^7.22.0",
        "@wordpress/components": "^29.8.0",
        "@wordpress/element": "^6.22.0",
        "@wordpress/i18n": "^5.22.0",
        "autoprefixer": "^10.4.21",
        "marked": "^15.0.12"
    },
    "devDependencies": {
        "@wordpress/dependency-extraction-webpack-plugin": "^6.22.0",
        "@wordpress/eslint-plugin": "^22.8.0",
        "@wordpress/prettier-config": "^4.22.0",
        "@wordpress/scripts": "^30.15.0",
        "eslint": "^9.25.1",
        "eslint-import-resolver-node": "0.3.9",
        "eslint-plugin-eslint-comments": "3.2.0",
        "prettier": "npm:wp-prettier@3.0.3",
        "tailwindcss": "^3.0.0",
        "vite": "^3.0.0"
    }
}
